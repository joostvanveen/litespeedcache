{
    "name": "joostvanveen/litespeedcache",
    "description": "A framework agnostic Litespeed cache library for PHP. Can be used in any PHP application.",
    "type": "library",
    "keywords": ["litespeed","cache","caching","fpc","full page cache","varnish","php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joost van Veen",
            "email": "joost@accentinteractive.nl"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Joostvanveen\\Litespeedcache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joostvanveen\\Litespeedcache\\Tests\\": "tests"
        }
    }
}
